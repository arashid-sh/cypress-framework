{
  "ts-node": {
    "files": true
  },
  "compilerOptions": {
    "allowJs": true,
    "target": "es5",
    "lib": ["es5", "dom"],
    "esModuleInterop": true,
    "module": "commonjs",
    "strictNullChecks": true,
    "noUnusedLocals": false,
    "resolveJsonModule": true,
    "noUnusedParameters": false,
    "moduleResolution": "node",
    "types": ["node", "cypress"]
  },
  "include": ["**/*.ts"]
}
